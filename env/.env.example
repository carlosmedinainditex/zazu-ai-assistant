# Copy this file as .env and edit the values according to your credentials

# JIRA server URL
JIRA_SERVER=https://jira.inditex.com/jira

# JIRA user credentials
JIRA_USER=your_username
JIRA_TOKEN=your_api_token

# Optional configuration
MAX_RESULTS=50
DEFAULT_PROJECT=MYPROJ
DEFAULT_JQL="project = MYPROJ AND status = 'In Progress'"
